{
  "name": "@nikitababko/id-generator",
  "version": "1.0.12",
  "description": "ID generator",
  "author": "Nikita Babko <niki.babko@gmail.com>",
  "scripts": {
    "start:dev": "ts-node src/index.ts",
    "start:example": "ts-node tests/index.ts",
    "clear": "rm -rf dist",
    "build": "tsc",
    "build:lib": "npm run clear && tsc -p tsconfig.production.json && webpack",
    "test:all": "mocha './src/index.spec.ts'",
    "pre:commit": "yarn test:all && yarn build:lib && yarn v:patch",
    "v:release": "npm --no-git-tag-version version prerelease && git add .",
    "v:patch": "npm --no-git-tag-version version patch && git add .",
    "v:minor": "npm --no-git-tag-version version minor && git add .",
    "v:major": "npm --no-git-tag-version version major && git add .",
    "prepare": "npx husky install",
    "create-hook": "npx husky add .husky/pre-commit \"yarn pre:commit && git add .\""
  },
  "keywords": [
    "id",
    "generator",
    "id generator",
    "id-generator",
    "crypto",
    "encryption"
  ],
  "sideEffects": false,
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/nikitababko/id-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikitababko/id-generator.git"
  },
  "bugs": {
    "url": "https://github.com/nikitababko/id-generator/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist",
    "LICENSE.md",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=14.17.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "chai": "^4.3.6",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  }
}
